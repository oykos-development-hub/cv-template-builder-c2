{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": ["error", { "endOfLine": "auto" }],
		"arrow-body-style": "off",
		"prefer-arrow-callback": "off"
	}
}
